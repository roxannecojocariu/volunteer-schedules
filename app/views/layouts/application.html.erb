<!DOCTYPE html>
<html>
  <head>
    <title>VolunteerSchedules</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "layouts/nav_bar" %>
    <%- flash.each do |key, value| -%>
    <div class="flash flash-<%= key %>">
      <%= value %>
    </div>
    <%- end -%>

    <%= yield %>

    <%= javascript_include_tag "application" %>

    <%= yield :extra_footer %>
  </body>
</html>
